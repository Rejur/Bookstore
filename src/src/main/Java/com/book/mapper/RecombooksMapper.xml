  </resultMap>
  <insert id="insert" parameterType="com.book.pojo.Recombooks">
    insert into RecomBooks (BookId, UserId)
    values (#{bookid,jdbcType=INTEGER}, #{userid,jdbcType=INTEGER})
  </insert>
  <insert id="insertSelective" parameterType="com.book.pojo.Recombooks">
    insert into RecomBooks
